<!doctype html>
<html class="fixed">

<head>
   
    <!-- Basic -->
    <meta charset="UTF-8">
    <meta name="keywords" content="" />
    <meta name="description" content="">
    <meta name="author" content="dragonviet">
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <!-- Web Fonts  -->
    <title>@ViewBag.Title</title>

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">
    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/Areas/AdminIT/Content/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Areas/AdminIT/Content/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/Areas/AdminIT/Content/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="~/Areas/AdminIT/Content/vendor/bootstrap-datepicker/css/datepicker3.css" />
    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/Areas/AdminIT/Content/stylesheets/theme.css" />
    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/Areas/AdminIT/Content/stylesheets/theme-custom.css">
    <!-- Head Libs -->
    <script src="~/Areas/AdminIT/Content/vendor/modernizr/modernizr.js"></script>
</head>
<body>
    <!-- start: page -->
    <section class="body-sign">
        <div class="center-sign">
            <a href="#" class="logo pull-left">
                <img src="~/Areas/AdminIT/Content/images/dragonviet.png" height="54" alt="Dragon Admin" />
            </a>
            <div class="panel panel-sign">
                @RenderBody()
            </div>
            <p class="text-center text-muted mt-md mb-md">&copy; Copyright 2015. All Rights Reserved.</p>
        </div>
    </section>
    <!-- end: page -->
    <!-- Vendor -->
    <script src="~/Areas/AdminIT/Content/vendor/jquery/jquery.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/jquery-cookie/jquery.cookie.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/style-switcher/style.switcher.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/magnific-popup/magnific-popup.js"></script>
    <script src="~/Areas/AdminIT/Content/vendor/jquery-placeholder/jquery.placeholder.js"></script>


    <script src="~/Areas/AdminIT/Scripts/theme.js"></script>
    <script src="~/Areas/AdminIT/Scripts/theme.custom.js"></script>
    <script src="~/Areas/AdminIT/Scripts/theme.init.js"></script>

</body>
